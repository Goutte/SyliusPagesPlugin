{% form_theme form '@AropixelSyliusPagesPlugin/Admin/Form/theme.html.twig' %}

<div class="thirteen wide column">
    {% include '@SyliusAdmin/Crud/form_validation_errors_checker.html.twig' %}

    <div >

        <div class="column">

            {{ form_row(form.code) }}
            {{ form_row(form.title) }}
